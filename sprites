def gpig():
    ass.dellslow("           .     .")
    ass.dellslow("           (>\---/<)")
    ass.dellslow("           ,'     `.")
    ass.dellslow("          /  q   p  \ ")
    ass.dellslow("         (  >(_Y_)<  ) ")
    ass.dellslow("          >-' `-' `-<-.")
    ass.dellslow("         /  _.== ==.,- \ ")
    ass.dellslow("        /,    )`  '(    )")
    ass.dellslow("       ; `._.'      `--<")
    ass.dellslow("      :     \        |  )")
    ass.dellslow("      \      )       ;_/ ")
    ass.dellslow("       `._ _/_  ___.'-\\\ ")
    ass.dellslow("          `--\\\ ")


